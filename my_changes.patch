+      error: (err: any) => console.error(err),
+      complete: () => {},
+      next: (data: object) => {
+        console.log('Message sent to websocket: ', data);
+        if (ws.readyState === WebSocket.OPEN) {
+          ws.send(JSON.stringify(data));
+        }
+      },
+    };
+    return new AnonymousSubject<MessageEvent>(observer, observable);
+  }
 }
